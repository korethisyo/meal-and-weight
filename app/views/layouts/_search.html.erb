<!--検索フォーム-->
<% if user_signed_in? %>
<div class="container mt-2 mb-2">
  <div class="row justify-content-center">
    <%= form_with url: search_path, method: :get, local:true do |f| %>
      <%= f.text_field :content %>
      <%= f.submit '検索', class:"btn btn-primary" %>
    <% end %>
  </div>
</div>
<% end %>
